<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InputSecretPass">
        <attr name="labelHint" format="reference"/>
        <attr name="with_eye" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="IconButton">
        <attr name="iconResImage" format="reference"/>
        <attr name="iconNameText" format="reference|string"/>
    </declare-styleable>
    <declare-styleable name="StepNatural">
        <attr name="stepRes" format="reference"/>
    </declare-styleable>
    <declare-styleable name="StepBar">
        <attr name="sizeSteps" format="integer"/>
        <attr name="initialStep" format="integer"/>
        <attr name="separationWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="InputNormal">
        <attr name="inputType" format="enum">
            <enum name="textNormal" value="0"/>
            <enum name="textEmail" value="1"/>
        </attr>
        <attr name="textHint" format="reference|string"/>
    </declare-styleable>
    <declare-styleable name="SpinerYg">
        <attr name="textHintSpinner" format="reference|string"/>
    </declare-styleable>
</resources>